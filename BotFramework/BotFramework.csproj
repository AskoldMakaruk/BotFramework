<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors />
    <Company>BotFramework</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>0.0.2.0</AssemblyVersion>
    <FileVersion>0.0.2.0</FileVersion>
    <Version>0.2.0</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/AskoldMakaruk/BotFramework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AskoldMakaruk/BotFramework</RepositoryUrl>
    <RepositoryType>Open</RepositoryType>
    <PackageTags>telegram bot</PackageTags>
    <PackageReleaseNotes>Oh I see what you did! You refactor it!</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.6.0" />
    <PackageReference Include="OptionalMonad" Version="1.0.1" />
    <PackageReference Include="System.Collections.Immutable" Version="1.7.0" />
    <PackageReference Include="Telegram.Bot" Version="15.1.0" />
  </ItemGroup>

</Project>
