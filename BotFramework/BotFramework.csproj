<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors />
    <Company>BotFramework</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>0.0.3.1</AssemblyVersion>
    <FileVersion>0.0.3.2</FileVersion>
    <Version>0.3.1-dev-3</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/AskoldMakaruk/BotFramework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AskoldMakaruk/BotFramework</RepositoryUrl>
    <RepositoryType>Open</RepositoryType>
    <PackageTags>telegram bot</PackageTags>
    <PackageReleaseNotes>Please don't use previous version.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.7.82" />
    <PackageReference Include="OptionalMonad" Version="2.1.0" />
    <PackageReference Include="Serilog" Version="2.9.0" />
    <PackageReference Include="Serilog.Sinks.Console" Version="3.1.1" />
    <PackageReference Include="Telegram.Bot" Version="15.2.0" />
  </ItemGroup>

</Project>
